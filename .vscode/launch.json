{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C#: IronWorkoutTracker.Presentation [http]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/IronWorkoutTracker.Presentation/bin/Debug/net8.0/IronWorkoutTracker.Presentation.dll",
      "args": [],
      "cwd": "${workspaceFolder}/IronWorkoutTracker.Presentation",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": "C#: IronWorkoutTracker.Presentation [https]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/IronWorkoutTracker.Presentation/bin/Debug/net8.0/IronWorkoutTracker.Presentation.dll",
      "args": [],
      "cwd": "${workspaceFolder}/IronWorkoutTracker.Presentation",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7240;http://localhost:5138"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
